            <organizationUrl>https://www.crowdcode.io</organizationUrl>
        </developer>
    </developers>

    <scm>
        <connection>scm:git:https://dev.crowdcode.io/bitbucket/scm/hslb/lbds-http</connection>
        <developerConnection>scm:svn:https://dev.crowdcode.io/bitbucket/scm/scm/hslb/lbds-http
        </developerConnection>
    </scm>

    <distributionManagement>
        <repository>
            <id>repo.crowdcode.io</id>
            <name>repo.crowdcode.io</name>
            <url>https://repo.crowdcode.io/repository/releases/</url>
        </repository>

        <snapshotRepository>
            <id>repo.crowdcode.io</id>
            <name>repo.crowdcode.io</name>
            <url>https://repo.crowdcode.io/repository/snapshots/</url>
        </snapshotRepository>
